package dsa1;

public class Reverseelementsarray {

	public static void main(String[] args) {

		int arr[] = {1,2,3,4 ,5,6,7,8};
		int start=0;
		int end=arr.length-1;
		   // Reverse the array
        while (start < end) {
            // Swap
            int temp = arr[start];
            arr[start] = arr[end];
            arr[end] = temp;
            start++;
            end--;
        }
		for(int num:arr) {
			System.out.print(num+" ");
		}
		
	}

}
